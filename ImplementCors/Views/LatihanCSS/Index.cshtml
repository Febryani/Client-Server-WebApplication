<div class="modal fade" id="showDetail" tabindex="-1" role="dialog" aria-labelledby="showDetailCenterTitle" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header bg-light">
                <h5 class="modal-title" id="exampleModalLabel">Details Information</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div id="personDetail">
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                @*  <button type="button" class="btn btn-primary">Save changes</button>*@
            </div>
        </div>
    </div>
</div>

<div class="modal fade" id="insertData" tabindex="-1" role="dialog" aria-labelledby="insertDataCenterTitle" aria-hidden="true">
    <div class="modal-dialog modal-lg modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header bg-light">
                <h5 class="modal-title text-center" id="exampleModalLabel">Registration Form</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div id="dataDetail">
                    <form class="was-validated" id="regisForm">
                        <div class="row">
                            <div class="col-sm-2" id="formgroup">
                                <label for="nik">NIK:</label>
                                <input type="text" class="form-control" id="nik" placeholder="123" name="nik" required />
                                <div class="valid-feedback">Valid.</div>
                                <div class="invalid-feedback">Please fill out this field.</div>
                            </div>
                            <div class="col-sm-5" id="formgroup">
                                <label for="firstname">Firstname:</label>
                                <input type="text" class="form-control" id="firstname" placeholder="Insert your firstname" name="firstname" required />
                                <div class="valid-feedback">Valid.</div>
                                <div class="invalid-feedback">Please fill out this field.</div>
                            </div>
                            <div class="col-sm-5" id="formgroup">
                                <label for="lastname">Lastname:</label>
                                <input type="text" class="form-control" id="lastname" placeholder="Insert your lastname" name="lastname" required />
                                <div class="valid-feedback">Valid.</div>
                                <div class="invalid-feedback">Please fill out this field.</div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-sm-4" id="formgroup">
                                <label>Birthdate:</label>
                                <input type="date" required name="bday" class="form-control" id="birthdate" />
                                <div class="valid-feedback">Valid.</div>
                                <div class="invalid-feedback">Please fill out this field.</div>
                            </div>
                            <div class="col-sm-3" id="formgroup">
                                <label for="inputGender">Gender:</label>
                                <select class="custom-select" id="gender" required>
                                    <option value=0>Male</option>
                                    <option value=1>Female</option>
                                </select>
                                <div class="valid-feedback">Check again!</div>
                            </div>
                            <div class="col-sm-5" id="formgroup">
                                <label for="phone">Phone Number:</label>
                                <input type="number" class="form-control" id="phonenumber" placeholder="Ex: 08123456789" name="phone" required />
                                <div class="valid-feedback">Valid.</div>
                                <div class="invalid-feedback">Please fill out this field.</div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-sm-4" id="formgroup">
                                <label for="inputUniv">University:</label>
                                <select class="custom-select" id="universityId" required>
                                    <option value=1>Stanford University</option>
                                    <option value=2>Harvard University</option>
                                    <option value=3>NTU</option>
                                    <option value=4>ITB</option>
                                    <option value=5>London University</option>
                                </select>
                                <div class="valid-feedback">Check again!</div>
                            </div>
                            <div class="col-sm-3" id="formgroup">
                                <label for="inputDegree">Degree:</label>
                                <select class="custom-select" id="degree" required>
                                    <option value="Bachelor Degree">Bachelor</option>
                                    <option value="Master Degree">Master</option>
                                    <option value="Doctoral Degree">Doctoral</option>
                                </select>
                                <div class="valid-feedback">Check again!</div>
                            </div>
                            <div class="col-sm-2" id="formgroup">
                                <label for="gpa">GPA:</label>
                                <input type="number" class="form-control" id="gpa" placeholder="4.00" name="gpa" min="0" max="4" step="0.01" required />
                                <div class="valid-feedback">Valid.</div>
                                <div class="invalid-feedback">Please fill out this field.</div>
                            </div>
                            <div class="col-sm-3" id="formgroup">
                                <label for="salary">Salary:</label>
                                <input type="number" class="form-control" id="salary" placeholder="IDR Ex: 100000" name="salary" required />
                                <div class="valid-feedback">Valid.</div>
                                <div class="invalid-feedback">Please fill out this field.</div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-sm-4" id="formgroup">
                                <label for="email">Email:</label>
                                <input type="email" class="form-control" id="email" placeholder="example@gmail.com" name="email" aria-describedby="emailHelp" required />
                                <div class="valid-feedback">Valid.</div>
                                <div class="invalid-feedback">Please fill out this field.</div>
                            </div>
                            <div class="col-sm-3" id="formgroup">
                                <label for="password">Password:</label>
                                <input type="password" class="form-control" id="password" placeholder="min 8 character" name="password" pattern=".{8,}" aria-hidden="true" required />
                                <div class="valid-feedback">Valid.</div>
                                <div class="invalid-feedback">Please fill out this field.</div>
                            </div>
                            <div class="col-sm-5" id="formgroup">
                                <label for="submit">Click for submit!</label>
                                <button type="submit" class="btn btn-primary col-sm-12" @*data-dismiss="modal"*@ id="submitData" @*onclick="Insert()"*@>SUBMIT</button>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal" onclick="Reset()">Close</button>
                @*  <button type="button" class="btn btn-primary">Save changes</button>*@
            </div>
        </div>
    </div>
</div>


